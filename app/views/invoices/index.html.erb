<%= render partial: 'shared/merchant_dashboard_nav', locals: { merchant: @merchant } %>

<div class="row">
  <div class="col-sm-12">
    <p class='col-head'>
      My Invoice
    </p>
  </div>

  <div class="col-sm-12">
    <% @invoices.each do |invoice| %>
      <p>
        <%= link_to "Invoice ##{invoice.id}", merchant_invoice_path(@merchant, invoice) %>
      </p>
    <% end %>
  </div>
</div>
